type=\"primary\" @click=\"submitForm\">提交</el-button>\n        <el-button @click=\"resetForm('UserInfo')\">重置</el-button>\n      </el-form-item>\n    </el-form>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      labelPosition: 'right',\n      formLabelAlign: {\n        name: '',\n        region: '',\n        type: ''\n      },\n      //用户信息\n      UserInfo: {\n        useranme: unescape(localStorage.getItem(\"username\")),\n        name: '',\n        credit: 0,\n        phone: '',\n        address: '',\n        password: unescape(localStorage.getItem(\"password\")),\n      },\n      UpdateStatus: false\n    };\n  },\n  methods: {\n    submitForm() {\n      console.log(this.UserInfo.useranme, this.UserInfo.password);\n      fetch(this.$store.state.URL + \"/User/Update/GarbageMan\", {\n        method: \"POST\",\n        headers: {\n          \"Content-Type\": \"application/json\",\n          \"Authorization\": \"Bearer \" + unescape(localStorage.getItem(\"token\"))\n        },\n        body: JSON.stringify(this.UserInfo),\n      }).then((response) => {\n        console.log(this.UserInfo);\n        let result = response.json();\n        result.then((res) => {\n              this.UpdateStatus = res.status;\n            }\n        )\n      })\n    },\n    resetForm(formName) {\n      this.$refs[formName].resetFields();\n    }\n  },\n  mounted() {\n    this.UpdateStatus = false;\n    fetch(this.$store.state.URL + \"/User/GetInformation/GarbageMan?req=\" + this.UserInfo.username, {\n      method: \"GET\",\n      headers: {\n        \"Authorization\": \"Bearer \" + unescape(localStorage.getItem(\"token\")),\n      },\n    }).then((response) => {\n      let result = response.json();\n      result.then((result) => {\n        this.UserInfo.name = result.name;\n        this.UserInfo.credit = result.credit;\n        this.UserInfo.phone = result.phonenumber;\n        this.UserInfo.address = result.address;\n      })\n    })\n  }\n}\n</script>\n\n<style>\n\n</style>\n"],"sourceRoot":""}]}