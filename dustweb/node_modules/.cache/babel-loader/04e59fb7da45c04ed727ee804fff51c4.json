EAGkB;AAFd,QAAA,KAAK,EAAC,MAEQ;AADd,QAAA,IAAI,EAAC;AACS,OAHlB,CATkB,EAalB,YAAA,CAOkB,0BAPlB,EAOkB;AANd,QAAA,KAAK,EAAC,OAMQ;AALd,QAAA,KAAK,EAAC,IAKQ;AAJd,QAAA,KAAK,EAAC;AAIQ,OAPlB,E;AAIa,QAAA,OAAO,EAAA,QAAA,CAChB,UADuB,KACvB;AAAA,iBADuB,CACvB,YAAA,CAA0E,oBAA1E,EAA0E;AAA9D,YAAA,OAAK,EAAA,iBAAA,MAAA;AAAA,qBAAE,QAAA,CAAA,QAAA,CAAS,KAAK,CAAC,GAAN,CAAU,MAAnB,CAAF;AAAA,aAAyD;AAA3B,YAAA,IAAI,EAAC;AAAsB,WAA1E,E;8BAA4D;AAAA,qBAAE,C,UAAA,CAAF;AAAA,a;;WAA5D,E,IAAA,E,WAAA,CADuB,CACvB;AAAA,SADgB,C;;OAJpB,CAbkB,CAvBlB;AAAA,K;;GAJF,E,CAAA,E,QAAA,C","sourcesContent":["<template>\n  <el-table\n      :data=\"tableData\"\n      style=\"width: 100%\"\n      border>\n    <el-table-column type=\"expand\">\n      <template #default=\"props\">\n        <el-form label-position=\"left\" inline class=\"demo-table-expand\">\n          <el-form-item label=\"垃圾编号\">\n            <span>{{ props.row.gar_id }}</span>\n          </el-form-item>\n          <el-form-item label=\"被处罚人\">\n            <span>{{ props.row.user_id }}</span>\n          </el-form-item>\n          <!--          <el-form-item label=\"检查员\">-->\n          <!--            <span>{{ props.row.id }}</span>-->\n          <!--          </el-form-item>-->\n          <el-form-item label=\"原因\">\n            <span>{{ props.row.reason }}</span>\n          </el-form-item>\n          <el-form-item label=\"处罚\">\n            <span>{{ props.row.punishment }}</span>\n          </el-form-item>\n          <el-form-item label=\"违规时间\">\n            <span>{{ props.row.violate_time }}</span>\n          </el-form-item>\n        </el-form>\n      </template>\n    </el-table-column>\n    <el-table-column\n        label=\"垃圾编号\"\n        prop=\"gar_id\">\n    </el-table-column>\n    <el-table-column\n        label=\"处罚\"\n        prop=\"punishment\">\n    </el-table-column>\n    <el-table-column\n        label=\"违规时间\"\n        prop=\"violate_time\">\n    </el-table-column>\n    <el-table-column\n        fixed=\"right\"\n        label=\"操作\"\n        width=\"100\">\n      <template #default=\"scope\">\n        <el-button @click=\"withdraw(scope.row.gar_id)\" type=\"text\" >撤回</el-button>\n      </template>\n    </el-table-column>\n  </el-table>\n</template>\n\n<script>\nimport {Base64} from 'js-base64';\n\nexport default {\n  data() {\n    return {\n      ThrowRecord: {\n        \"gar_id\": \"11111116\",\n        \"user_id\": \"1952108\",\n        \"watcher_id\": \"\",\n        \"reason\": \"234\",\n        \"punishment\": 2,\n        \"violate_time\": \"2021-07-21T17:43:19\"\n      },\n      showDetail: false,\n      tableData: []\n    }\n  },\n  methods:{\n    getData()\n    {\n      const url = this.$store.state.URL + \"/ViolateRecord/GetAll?req=\" + Base64.decode(localStorage.username);\n      fetch(url, {\n        method: \"GET\",\n        headers: {\n          accept: \"text/plain\",\n          Authorization: \"Bearer \" + Base64.decode(localStorage.token),\n        },\n      }).then((response) => {\n        console.log(response);\n        let result = response.json();\n        result.then((data) => {\n          console.log(data);\n          this.tableData = data;\n        });\n      })\n    },\n    withdraw(req)\n    {\n      fetch(this.$store.state.URL + \"/ViolateRecord/Delete?req=\" + req, {\n        method: \"GET\",\n        headers: {\n          accept: \"text/plain\",\n          Authorization: \"Bearer \" + Base64.decode(localStorage.token),\n        },\n      }).then((response) => {\n        console.log(response);\n        let result = response.json();\n        result.then((data) => {\n          console.log(data);\n          this.getData();\n        });\n      })\n    }\n  },\n  mounted() {\n    console.log(1);\n    console.log(localStorage.token);\n    console.log(Base64.decode(localStorage.username));\n    this.getData();\n  }\n}\n</script>\n\n<style>\n.demo-table-expand {\n  font-size: 0;\n}\n\n.demo-table-expand label {\n  width: 90px;\n  color: #99a9bf;\n}\n\n.demo-table-expand .el-form-item {\n  margin-right: 0;\n  margin-bottom: 0;\n  width: 50%;\n}\n</style>\n"],"sourceRoot":""}]}