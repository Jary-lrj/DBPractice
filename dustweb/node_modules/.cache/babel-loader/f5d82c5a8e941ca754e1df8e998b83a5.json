en),\n        },\n      }).then((response) => {\n        console.log(response);\n        let result = response.json();\n        result.then((data) => {\n          console.log(data);\n          this.getData();\n        });\n      })\n    }\n  },\n  mounted() {\n    console.log(1);\n    console.log(localStorage.token);\n    console.log(Base64.decode(localStorage.username));\n    this.getData();\n  }\n}\n</script>\n\n<style>\n.demo-table-expand {\n  font-size: 0;\n}\n\n.demo-table-expand label {\n  width: 90px;\n  color: #99a9bf;\n}\n\n.demo-table-expand .el-form-item {\n  margin-right: 0;\n  margin-bottom: 0;\n  width: 50%;\n}\n</style>\n"],"sourceRoot":""}]}